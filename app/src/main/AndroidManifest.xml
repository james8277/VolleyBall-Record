<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="james.volleyballrecord">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity_AppMenu"
            android:label="@string/app_name"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Activity_InitialSet"
            android:label="@string/title_activity_initial_set"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".PlayGame"
            android:label="@string/title_activity_play_game"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Activity_Start"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Record"
            android:label="@string/title_activity_record"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>